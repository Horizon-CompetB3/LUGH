        </div>
        <div id="form-ins">
            <h2>Inscription</h2>
            <div class="form-group-ins">
                <label for="user_username" class="required">Nom</label>
                <input type="text" id="user_username" name="user[username]" required="required"/>
            </div>

            <div class="form-group-ins">
                <label for="user_email" class="required">E-mail</label>
                <input type="email" id="user_email" name="user[email]" required="required"/></div>

            <div class="form-group-ins">
                <label for="user_plainPassword_first" class="required">Mot de passe</label>
                <input type="password" id="user_plainPassword_first" name="user[plainPassword][first]"
                       required="required"/>
            </div>

            <div class="form-group-ins">
                <label for="user_plainPassword_second" class="required">Répéter le mot de passe</label>
                <input type="password" id="user_plainPassword_second" name="user[plainPassword][second]"
                       required="required"/>
            </div>

            <div class="form-group-ins" id="check-box">
                <div class="div-check-box">
                    <input type="checkbox" id="user_entreprise" name="user[entreprise]" required="required"
                           class="form-control" value="1"/>
                    <label for="user_entreprise" class="required">Je suis une entreprise</label>

                </div>
                <div class="div-check-box">
                    <input type="checkbox" id="user_artiste" name="user[artiste]" required="required"
                           class="form-control" value="0"/>
                    <label for="user_artiste" class="required">Je suis un(e) artiste</label>

                </div>
            </div>


            <div class="form-group-ins">
                <button type="submit" id="user_save" name="user[save]" class="save">S'INSCRIRE</button>
            </div>
            <input type="hidden" id="user__token" name="user[_token]"/>
            </form>
        </div>
    </section>
{% endblock %}